{
  "name": "vue-c2c",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@7.30.0",
  "description": "Transforming Vue components to composable functions",
  "author": "webfansplz",
  "license": "MIT",
  "homepage": "https://github.com/webfansplz/vue-c2c#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfansplz/vue-c2c.git"
  },
  "bugs": {
    "url": "https://github.com/webfansplz/vue-c2c/issues"
  },
  "keywords": [
    "components",
    "composbales",
    "vue3",
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "nr -C playground dev",
    "play:example": "nr -C examples/c2c dev",
    "play:example2": "nr -C examples/c2c-with-placeholder dev",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@antfu/ni": "^0.21.2",
    "@antfu/utils": "^0.7.2",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/test-utils": "^2.3.2",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "esno": "^0.16.3",
    "happy-dom": "^9.5.0",
    "lint-staged": "^13.2.0",
    "pnpm": "^8.1.1",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0",
    "vite": "^4.2.1",
    "vitest": "^0.29.8",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
